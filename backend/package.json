{
  "name": "backend",
  "version": "1.0.0",
  "description": "ReadME Generator Backend",
  "main": "./dist/server.js",
  "author": "ShaanCoding",
  "license": "private",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "npx tsc",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/src/server.js\"",
    "dev-reload-swagger": "concurrently \"npx tsc --watch\" \"nodemon -q dist/src/server.js\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": ""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/swagger-ui-express": "^4.1.6",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "typescript": "^5.3.2"
  }
